{
  "env" : {
    "browser" : true,
    "node" : true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "parser": "babel-eslint",
    "ecmaFeatures": {
      "experimentalObjectRestSpread":true,
      "jsx": true
    }
  },
  "rules": {
    "complexity": [2, 6]
  },
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}